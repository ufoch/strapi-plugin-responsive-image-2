{
  "version": "1.0.15",
  "keywords": [
    "strapi",
    "plugin",
    "responsive",
    "image",
    "media"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.14",
    "@strapi/icons": "^2.0.0-rc.14",
    "react-intl": "^7.0.4"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.8",
    "@strapi/strapi": "^5.5.1",
    "@strapi/typescript-utils": "^5.5.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/react-helmet": "^6.1.11",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.2",
    "styled-components": "^6.1.13",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.8",
    "@strapi/strapi": "^5.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "responsive-image-2",
    "displayName": "Responsive Image 2",
    "description": "Custom responsive image formats for strapi. Successor of abandoned strapi-plugin-responsive-image."
  },
  "name": "strapi-plugin-responsive-image-2",
  "description": "Custom responsive image formats for strapi. Successor of abandoned strapi-plugin-responsive-image.",
